{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "react redux boilerplate",
  "main": "lib/index.js",
  "scripts": {
    "build": "./node_modules/.bin/babel src --out-dir lib",
    "example": "./node_modules/.bin/webpack -p --config webpack.dll.config.js && ./node_modules/.bin/webpack -p --config webpack.config.js",
    "example:dll": "NODE_ENV=development ./node_modules/.bin/webpack --progress --colors --config webpack.dll.config.js",
    "start": "NODE_ENV=development node server.js",
    "lint": "./node_modules/.bin/eslint 'components/**/*.js' 'containers/**/*.js' 'modules/**/*.js' 'app/**/*.js'",
    "analyzer": "./node_modules/.bin/webpack --json | webpack-bundle-size-analyzer",
    "prepublish": "npm run build",
    "gh-pages": "make gh-pages"
  },
  "author": "Yulong Ruan",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "classnames": "^2.2.5",
    "react": "^15.4.2",
    "react-dnd": "^2.4.0",
    "react-dnd-html5-backend": "^2.4.1",
    "react-dom": "^15.4.2",
    "react-virtualized": "^9.9.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.3",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.3.2",
    "babel-preset-env": "^1.1.8",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "express": "^4.14.1",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "file-loader": "^0.10.0",
    "html-webpack-plugin": "^2.28.0",
    "node-sass": "^4.5.0",
    "postcss-loader": "^1.3.1",
    "postcss-smart-import": "^0.6.9",
    "precss": "^1.4.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "sass-loader": "^6.0.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-bundle-size-analyzer": "^2.5.0",
    "webpack-dev-server": "^2.4.1",
    "webpack-merge": "^3.0.0"
  }
}
